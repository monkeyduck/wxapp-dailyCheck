}



.txt-btn .audio-3{
  margin: 0 auto;
  overflow: hidden;
  width:600rpx;
  height: 72rpx;
  background: #F0F0F0;
  color: #666;
  font-size: 28rpx;
  border-radius: 4rpx;
}
.txt-btn .audio-3>view{
width: 50%;
height: 100%;
line-height: 68rpx;
float: left;
font-size: 28rpx;
position: relative;
}
.txt-btn .audio-3 .playaudio{
text-indent: 1.3em;
}
.txt-btn .audio-3 .playaudio::before{
content: '';
display: block;
height: 0;
width: 0;
border: 14rpx solid transparent;
border-left-color: #999; 
position: absolute;
left: 36%;
top: 50%;
margin-top: -14rpx;
}
.txt-btn .audio-3 .playaudio.on::before{
height: 24rpx;
width: 8rpx;
border: 4rpx solid #999;
border-top: none; 
border-bottom: none; 
margin-top: -12rpx;
}
.txt-btn .audio-3 .reset::before{
content: '';
display: block;
height: 100%;
width: 0;
border-left: 2rpx solid #ccc;
position: absolute;
left: 0;
top: 0;
}


